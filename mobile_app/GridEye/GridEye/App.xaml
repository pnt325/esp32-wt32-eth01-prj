<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WT32EHT01.App">
    <Application.Resources>
        <Color x:Key="colorFriwoForeground">#1b62b7</Color>
        <Color x:Key="colorFriwoBackground">#fcc707</Color>
        <Color x:Key="colorDarkTransperent">#80000000</Color>
        <Color x:Key="colorWindowBackground">#fafafa</Color>
        <Style TargetType="NavigationPage">
            <Setter Property="BarBackgroundColor" Value="{StaticResource colorFriwoBackground}"/>
            <Setter Property="BarTextColor" Value="{StaticResource colorFriwoForeground}"/>
        </Style>
    </Application.Resources>
</Application>